package day0811;
import java.util.Scanner;

public class B2869_1 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		int b = s.nextInt();
		int v = s.nextInt();
		
		v = v-a;
		int day;
		if((day=v/(a-b))!=0)
			day +=2;
		else 
			day +=1;
		System.out.println(day);
		
		
		
		

	}

}
