package day0811;

import java.util.Scanner;

public class B1541 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		String str = s.nextLine();
		int plus = str.indexOf("+");
		int minus = str.indexOf("-");
		for(int i=0; i<str.length(); i++) {
			if(str.charAt(i)=='+')
				System.out.println("/ +:"+i);
			if(str.charAt(i)=='-')
				System.out.println("/ -:"+i);
		}
	}

}
