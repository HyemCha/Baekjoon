package day0805;

import java.util.Scanner;

public class B1110 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int num = s.nextInt();
		if (!(num>=0 && num<=99))
			return;
		int[] a = new int[2];
		int original = num, f=0;
		int newNum;
		while(true){
			if(f == 0){
			
			f += 1;
			
			if(num == original){
				System.out.println(f);
				break;
			}
		}
	}

}
