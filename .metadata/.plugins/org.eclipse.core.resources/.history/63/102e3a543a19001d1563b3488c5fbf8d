package day0811;
import java.util.Scanner;

public class B2869_1 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		int b = s.nextInt();
		int v = s.nextInt();
		
		int day = (v-a)/(a-b) + 1;
		if((v-a)%(a-b)!=0)
			day++; 
		System.out.println(day);
	}

}
