import java.util.Scanner;

public class B2869 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		int b = s.nextInt();
		int v = s.nextInt();
		
		int cnt = 0;
		
		while(true) {
			if(v<=a) {
				cnt++;
				break;
			}
			v -= (a-b);
			cnt++;
		}
		System.out.println(cnt);
		
	}

}
